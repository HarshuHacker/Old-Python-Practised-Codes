    "def swapAlternate(arr, n) :\n",
    "    #Your code goes here\n",
    "    n=(n//2)*2\n",
    "    for i in range(0,n,2):\n",
    "        for j in range(i,i+1,1):\n",
    "            temp=arr[i]\n",
    "            arr[i]=arr[i+1]\n",
    "            arr[i+1]=temp\n",
    "n=6\n",
    "arr=[1,2,3,4,5,6]\n",
    "swapAlternate(arr, n)\n",
    "print(arr)"
   ]
