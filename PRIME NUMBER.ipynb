     "text": [
      "71\n",
      "Number Is Prime\n"
     ]
    }
   ],
   "source": [
    "n=int(input())\n",
    "count=2\n",
    "flag=False\n",
    "while(count<n):\n",
    "    if(n%count==0):\n",
    "        print(\"Number Is Not Prime\")\n",
    "        flag=True\n",
    "        count=n\n",
    "    else:\n",
    "        count=count+1\n",
    "if(flag==False):\n",
    "    print(\"Number Is Prime\")"
   ]
  }
